@model GestorUsuarios.Models.Tecnico

@{
    ViewBag.Title = "Eliminar Técnico";
}

<h2>Eliminar Técnico</h2>

<p>¿Estás seguro de que deseas eliminar este técnico?</p>

<dl class="dl-horizontal">
    <dt>Nombre:</dt>
    <dd>@Model.Nombre</dd>
    <dt>Especialidad:</dt>
    <dd>@Model.Especialidad</dd>
</dl>

@using (Html.BeginForm("DeleteConfirmed", "Tecnicos", new { id = Model.Id }, FormMethod.Post))
{
    <button type="submit" class="btn btn-danger">Eliminar</button>
    <a href="@Url.Action("Index")" class="btn btn-secondary">Cancelar</a>
}
